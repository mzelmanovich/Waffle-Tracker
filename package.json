{
  "name": "waffle-tracker",
  "version": "1.0.0",
  "description": "Music App",
  "main": "./build/index.js",
  "scripts": {
    "docker:build": "docker-compose --verbose -f docker-compose.yml build ",
    "docker:dev": "docker-compose -f docker-compose.yml up web-dev-server",
    "docker:unit-tests": "docker-compose -f docker-compose.yml up unit-tests",
    "docker:unit-tests:debug": "docker-compose -f docker-compose.yml up unit-tests-debug",
    "test": "ts-node ./node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "test:debug": "node --inspect-brk=0.0.0.0 --nolazy -r ts-node/register ./node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "start:dev": "./node_modules/.bin/webpack-dev-server",
    "webpack": "./node_modules/webpack/bin/webpack.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mzelmanovich/Waffle-Tracker.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mzelmanovich/Waffle-Tracker/issues"
  },
  "homepage": "https://github.com/mzelmanovich/Waffle-Tracker#readme",
  "devDependencies": {
    "@types/jasmine": "^3.5.10",
    "@types/node": "^13.9.1",
    "jasmine": "^3.5.0",
    "jasmine-ts": "^0.3.0",
    "nyc": "^15.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-ts-console-reporter": "^3.1.1"
  }
}
