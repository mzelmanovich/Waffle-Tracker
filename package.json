{
  "name": "waffle-tracker",
  "version": "1.0.0",
  "description": "Music App",
  "main": "./build/index.js",
  "scripts": {
    "docker:build": "docker-compose --verbose -f docker-compose.yml build ",
    "docker:dev": "docker-compose -f docker-compose.yml up web-dev-server",
    "docker:lint": "docker-compose -f docker-compose.yml up --exit-code-from lint lint" ,
    "docker:lint:fix": "docker-compose -f docker-compose.yml up lint-fix",
    "docker:unit-tests": "docker-compose -f docker-compose.yml up --exit-code-from unit-test unit-tests",
    "docker:unit-tests:debug": "docker-compose -f docker-compose.yml up unit-tests-debug",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "ts-node ./node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "test:debug": "node --inspect-brk=0.0.0.0 --nolazy -r ts-node/register ./node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "start:dev": "./node_modules/.bin/webpack-dev-server",
    "webpack": "./node_modules/webpack/bin/webpack.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mzelmanovich/Waffle-Tracker.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mzelmanovich/Waffle-Tracker/issues"
  },
  "homepage": "https://github.com/mzelmanovich/Waffle-Tracker#readme",
  "devDependencies": {
    "@types/jasmine": "^3.5.10",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "jasmine": "^3.5.0",
    "jasmine-ts": "^0.3.0",
    "nyc": "^15.0.0",
    "prettier": "^2.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-ts-console-reporter": "^3.1.1"
  }
}
